{
  "$id": "https://dev.liquality.com/schema/block.json",
  "title": "Block",
  "description": "Blockchain block",
  "type": "object",
  "required": ["number", "hash", "timestamp", "size"],
  "properties": {
    "hash": {
      "type": "string",
      "title": "Hash"
    },
    "number": {
      "type": "number",
      "title": "Number",
      "minimum": 0
    },
    "timestamp": {
      "type": "number",
      "title": "Timestamp",
      "minimum": 0
    },
    "difficulty": {
      "type": "number",
      "title": "Difficulty",
      "minLength": 1
    },
    "size": {
      "type": "number",
      "title": "Size",
      "minimum": 0
    },
    "parentHash": {
      "type": "string",
      "title": "Parent Hash"
    },
    "nonce": {
      "type": "number",
      "title": "Nonce",
      "minimum": 0
    },
    "_raw": {
      "type": "object"
    }
  }
}
