{
  "$id": "https://dev.liquality.com/schema/transaction.json",
  "title": "Transaction",
  "description": "Blockchain transaction",
  "type": "object",
  "required": ["hash", "value"],
  "properties": {
    "blockHash": {
      "type": "string",
      "title": "Block Hash"
    },
    "blockNumber": {
      "type": "number",
      "title": "Block Number",
      "minimum": 0
    },
    "hash": {
      "type": "string",
      "title": "Transaction Hash"
    },
    "value": {
      "type": "number",
      "title": "Value",
      "minimum": 0
    },
    "confirmations": {
      "type": "number",
      "title": "Confirmations",
      "minimum": 0
    },
    "feePrice": {
      "type": "number",
      "title": "Fee Price",
      "minimum": 0
    },
    "fee": {
      "type": "number",
      "title": "Fee",
      "minimum": 0
    },
    "_raw": {
      "type": "object"
    }
  }
}
